{
  "name": "ozankurt/fluent",
  "description": "Standalone Version of Laravel's Fluent Helper",
  "keywords": ["laravel", "fluent"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ozan Kurt",
      "email": "me@ozankurt.com",
      "homepage": "https://ozankurt.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0"
  },
  "autoload": {
    "psr-4": {
      "OzanKurt\\Fluent\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OzanKurt\\Fluent\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
